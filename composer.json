{
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "easycorp/easyadmin-bundle": "^1.17",
        "facebook/graph-sdk": "^5.7",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "friendsofsymfony/user-bundle": "^2.1",
        "geocoder-php/google-maps-provider": "^4.2",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "hwi/oauth-bundle": "^0.6",
        "jms/serializer-bundle": "^2.4",
        "knplabs/knp-paginator-bundle": "^3.0",
        "lexik/maintenance-bundle": "^2.1",
        "liip/imagine-bundle": "^2.1",
        "neitanod/forceutf8": "^2.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.13",
        "presta/sitemap-bundle": "^1.5",
        "ricardoper/twitteroauth": "2.*",
        "sensio/framework-extra-bundle": "^5.2",
        "social-links/social-links": "^1.13",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/css-selector": "4.2.*",
        "symfony/doctrine-bridge": "4.2.*",
        "symfony/dom-crawler": "4.2.*",
        "symfony/event-dispatcher": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.2",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/http-foundation": "4.2.*",
        "symfony/inflector": "4.2.*",
        "symfony/intl": "4.2.*",
        "symfony/monolog-bridge": "4.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/process": "4.2.*",
        "symfony/routing": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/stopwatch": "4.2.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.2.*",
        "symfony/twig-bridge": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/yaml": "4.2.*",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.8",
        "whiteoctober/breadcrumbs-bundle": "^1.4",
        "willdurand/geocoder-bundle": "^5.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "4.2.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
