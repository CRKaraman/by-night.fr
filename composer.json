{
    "name":  "gsa/bynight",
    "description": "By Night",
    "homepage": "https://by-night.fr",
    "type": "project",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr",
            "homepage": "https://sainthillier.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-apcu": "*",
        "beberlei/doctrineextensions": "^1.2",
        "easycorp/easyadmin-bundle": "^2.1",
        "facebook/graph-sdk": "^5.7",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "friendsofsymfony/user-bundle": "^2.1",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.5",
        "hwi/oauth-bundle": "^0.6",
        "jms/serializer-bundle": "^3.2",
        "knplabs/knp-paginator-bundle": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-bundle": "^1.2",
        "league/glide-symfony": "^1.0",
        "neitanod/forceutf8": "^2.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "php-http/guzzle6-adapter": "^1",
        "php-http/httplug-bundle": "^1.15",
        "presta/sitemap-bundle": "^1.6",
        "ricardoper/twitteroauth": "2.*",
        "sensio/framework-extra-bundle": "^5.2",
        "sentry/sentry-symfony": "^3.0",
        "social-links/social-links": "^1.13",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/css-selector": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/dom-crawler": "*",
        "symfony/dotenv": "*",
        "symfony/event-dispatcher": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.2",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-foundation": "*",
        "symfony/inflector": "*",
        "symfony/intl": "*",
        "symfony/monolog-bridge": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/process": "*",
        "symfony/routing": "*",
        "symfony/security-bundle": "*",
        "symfony/stopwatch": "*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "*",
        "symfony/twig-bridge": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "*",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.6",
        "twistor/flysystem-http": "^0.2.0",
        "vich/uploader-bundle": "^1.9",
        "whiteoctober/breadcrumbs-bundle": "^1.4"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
