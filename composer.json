        "php-amqplib/rabbitmq-bundle": "dev-master",
        "php-http/guzzle6-adapter": "1.*",
        "php-http/httplug-bundle": "1.*",
        "presta/sitemap-bundle": "dev-master",
        "ricardoper/twitteroauth": "2.*",
        "social-links/social-links": "1.*",
