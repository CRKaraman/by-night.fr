{{ fos_httpcache_tag('detail-place-' ~ place.id ) }}
{% if page == 1 and soirees|length == 0 %}
    <p>Aucun événement prévu à {{ place.nom }} !</p>
{% else %}
{% include "@TBNAgenda/Agenda/block_preview_soiree.html.twig" with({"events": soirees}) %}
{% if hasNextLink %}
    <a class="more-content btn btn-primary btn-raised btn-block" href="{{ hasNextLink }}">Plus ({{ current }} / {{ count }})</a>
{% endif %}
{% endif %}
